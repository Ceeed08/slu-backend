{
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://www.slu-laboratory-resources.engineering",
        "Access-Control-Allow-Methods": "POST, GET, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization"
      },
      "status": 204
    },
    {
      "src": "/api/auth",
      "methods": ["POST"],
      "headers": {
        "Access-Control-Allow-Origin": "https://www.slu-laboratory-resources.engineering",
        "Access-Control-Allow-Methods": "POST, GET, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization"
      },
      "dest": "/api/auth.js"
    }
  ]
}