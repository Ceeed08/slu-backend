{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/index" }
  ],
  "routes": [
    {
      "src": "/api/auth",
      "methods": ["OPTIONS"],
      "status": 204,
      "headers": {
        "Access-Control-Allow-Origin": "https://www.slu-laboratory-resources.engineering",
        "Access-Control-Allow-Methods": "POST, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization"
      }
    },
    {
      "src": "/api/auth",
      "methods": ["POST"],
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/items/itemList",
      "methods": ["OPTIONS"],
      "status": 204,
      "headers": {
        "Access-Control-Allow-Origin": "https://www.slu-laboratory-resources.engineering",
        "Access-Control-Allow-Methods": "GET, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Authorization"
      }
    },
    {
      "src": "/api/items/itemList",
      "methods": ["GET"],
      "dest": "/api/items/itemList.js"
    }
  ]
}