{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Credentials", "value": "true" },
        { "key": "Access-Control-Allow-Origin", "value": "https://www.slu-laboratory-resources.engineering" },
        { "key": "Access-Control-Allow-Methods", "value": "GET, POST, PUT, DELETE, OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "X-Requested-With, Content-Type, Authorization, Accept" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/get-users",
      "destination": "/api/get-users.js"
    },
    {
      "source": "/api/update-user/:id",
      "destination": "/api/update-user.js"
    }
  ]
}
